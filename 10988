#include <iostream>
#include <string>
#include <algorithm>

bool is_palindrome(const std::string& s) {
	std::string rev;
	std::string::const_reverse_iterator itr = s.crbegin();

	while (itr != s.crend()) {
		rev = rev.append(1, *(itr++));
	};

	if (s == rev) 
		return 1;
	else 
		return 0;
}

int main() {
	std::string s;
	std::cin >> s;
	std::cout << is_palindrome(s) << std::endl;
}
